@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>roleDef</title>
</head>
<body>
    
    <h2>The example of role_def.</h2>

    <script>
        var obj = JSON.parse('{ "name":"John", "age":30, "city":"New York"}');
        document.getElementById("demo").innerHTML = obj.name + ", " + obj.age;
    </script>

    <p id="demo"></p>
</body>
</html>
